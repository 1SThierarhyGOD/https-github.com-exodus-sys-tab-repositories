<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DefineConstants>$(DefineConstants);KESTREL</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="$(SharedSourceRoot)runtime\Http3\**\*.cs" LinkBase="Shared\Http3" />
    <Compile Include="$(SharedSourceRoot)runtime\IHttpStreamHeadersHandler.cs" LinkBase="Shared\IHttpStreamHeadersHandler.cs" />
    <Compile Include="$(SharedSourceRoot)runtime\SR.cs" LinkBase="Shared\SR.cs" />
    <Compile Include="$(SharedSourceRoot)Http3cat\**\*.cs" LinkBase="Shared\Http3cat" />
    <Compile Include="$(SharedSourceRoot)ServerInfrastructure\*.cs" LinkBase="Shared\" />
    <Compile Include="$(SharedSourceRoot)PseudoHeaderNames.cs" Link="PseudoHeaderNames.cs" />
    <Compile Include="$(SharedSourceRoot)ValueTaskExtensions\*.cs" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Microsoft.AspNetCore.Server.Kestrel.Transport.Sockets" />
    <Reference Include="Microsoft.AspNetCore.Server.Kestrel.Core" />
    <Reference Include="Microsoft.Extensions.Hosting" />
    <Reference Include="Microsoft.Extensions.Logging.Console" />
    <Reference Include="Microsoft.AspNetCore.Testing" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="C:\aspnetcore\src\Shared\ServerInfrastructure\SharedStrings.resx" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="$(SharedSourceRoot)ServerInfrastructure\SharedStrings.resx" Link="Shared\SharedStrings.resx">
      <ManifestResourceName>Microsoft.AspNetCore.Server.SharedStrings</ManifestResourceName>
      <Generator></Generator>
    </EmbeddedResource>
    <EmbeddedResource Remove="C:\aspnetcore\src\Shared\runtime\SR.resx" />
    <EmbeddedResource Include="$(SharedSourceRoot)runtime\SR.resx" Link="Shared\runtime\SR.resx">
      <ManifestResourceName>System.Net.Http.SR</ManifestResourceName>
      <Generator></Generator>
    </EmbeddedResource>
  </ItemGroup>

</Project>

