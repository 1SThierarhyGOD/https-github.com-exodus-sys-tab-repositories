<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>ASP.NET Core middleware for caching HTTP responses on the server.</Description>
    <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
    <IsAspNetCoreApp>true</IsAspNetCoreApp>
    <IsPackable>false</IsPackable>
    <IsTrimmable>true</IsTrimmable>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.Extensions.Caching.Abstractions" />
    <Reference Include="Microsoft.Extensions.Caching.Memory" />
    <InternalsVisibleTo Include="Microsoft.Extensions.Caching.Distributed.Tests" />
    <Compile Include="..\..\..\Middleware\OutputCaching\src\RecyclableArrayBufferWriter.cs" Link="RecyclableArrayBufferWriter.cs" />
  </ItemGroup>
</Project>
